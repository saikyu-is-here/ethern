    "user_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "title": {
      "type": "string"
    },
    "message": {
      "type": "text"
    },
    "statu": {
      "type": "enumeration",
      "enum": [
        "unread",
        "read",
        "archived"
      ]
    },
    "time": {
      "type": "datetime"
    }
  }
}
